    "sql-template-strings": "^2.2.2",
    "swr": "^0.3.0",
    "undux": "^5.2.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@next/bundle-analyzer": "^9.5.1",
    "@testing-library/cypress": "^6.0.0",
    "ag-grid-community": "^23.2.1",
    "ag-grid-react": "^23.2.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^4.12.1",
    "cypress-social-logins": "^1.4.3",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.0.5",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "tabWidth": 2,
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "npm run prettier:fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
